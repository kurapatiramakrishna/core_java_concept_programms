class Circle 
{
	public double pi=3.14;
	public int r;
	public Circle(int a)
	{
	r=a;
	}
	public void  findArea()
	{
		double area=pi*r*r;
		System.out.println("The area of the circle is ="+area);
	}
}
class Circle2
{
	public static void main(String[] args) 
	{
		Circle c1=new Circle(12);
	    Circle c2=new Circle(22);
	    Circle c3=new Circle(33);
		c1.findArea();
		c2.findArea();
		c3.findArea();
	}
}